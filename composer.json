{
  "name": "kennersoft/skeleton-pim-new",
  "description": "Skeleton for a new KennerPIM",
  "homepage": "https://kennerpim.de",
  "license": "GPL-3.0-only",
  "minimum-stability": "stable",
  "require": {
    "kennerpim/core": "*",
    "kennerpim/pim": "*"
  },
  "scripts": {
    "pre-update-cmd": "ComposerCmd::preUpdate",
    "post-update-cmd": "ComposerCmd::postUpdate",
    "post-package-install": "ComposerCmd::postPackageInstall",
    "post-package-update": "ComposerCmd::postPackageUpdate",
    "pre-package-uninstall": "ComposerCmd::prePackageUninstall"
  },
  "autoload": {
    "classmap": [
      "composer-cmd.php"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.kennersoft.de/packages.json?id=public"
    }
  ]
}
